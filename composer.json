    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rennokki\\LaravelSnsEvents\\LaravelSnsEventsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.5"
    }
