{
    "name": "rennokki/laravel-sns-events",
    "description": "A description for laravel-sns-events.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://twitter.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
